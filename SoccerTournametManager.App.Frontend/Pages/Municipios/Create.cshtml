@page
@using SoccerTournametManager.App.Dominio
@model SoccerTournametManager.App.Frontend.Pages.Municipios.CreateModel
@{
}
<h1>Nuevo Municipio</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="municipio.Nombre"></label>
        <input asp-for="municipio.Nombre" class="form-control">
        <span asp-validation-for="municipio.Nombre" class="text-danger" ></span>
    </div>
    <button type="submit" class="btn btn-primary">Crear Municipio</button>
</form>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>