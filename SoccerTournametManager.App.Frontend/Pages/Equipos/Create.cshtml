@model SoccerTournametManager.App.Frontend.Pages.Equipos.CreateModel
@{
}
<h1>Nuevo equipo</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="equipo.Nombre"></label>
        <input asp-for="equipo.Nombre" class="form-control">
        <span asp-validation-for="equipo.Nombre" class="text-danger" ></span>
    </div>
    <button type="submit" class="btn btn-primary">Crear equipo</button>
</form>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>