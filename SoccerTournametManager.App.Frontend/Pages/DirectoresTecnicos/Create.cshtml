@page
@model SoccerTournametManager.App.Frontend.Pages.DirectoresTecnicos.CreateModel
@{
}
<h1>Nuevo Director Tecnico</h1>
<form method="post">
    <div class="form-group">
        <label asp-for="directortecnico.Nombre">Nombre</label>
        <input asp-for="directortecnico.Nombre" class="form-control">
        <span asp-validation-for="directortecnico.Nombre" class="text-danger" ></span> 
    </div>
     <div class="form-group">
        <label asp-for="directortecnico.Documento">Documento</label>
        <input asp-for="directortecnico.Documento" class="form-control">
        <span asp-validation-for="directortecnico.Documento" class="text-danger" ></span> 
    </div>
     <div class="form-group">
        <label asp-for="directortecnico.Telefono">Telefono</label>
        <input asp-for="directortecnico.Telefono" class="form-control">
        <span asp-validation-for="directortecnico.Telefono" class="text-danger" ></span> 
    </div>
    <button type="submit" class="btn btn-primary">Crear Director Tecnico</button>
</form>
<a class="btn btn-secondary" asp-page="./Index">Volver</a>
